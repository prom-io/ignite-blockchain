    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@types/supertest": "^2.0.8",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.0",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
